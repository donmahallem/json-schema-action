    "postlint:fix": "prettier --config ./.prettierrc src/**/*.ts --write",
    "docs": "typedoc --options ./typedoc.json"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "private": true,
  "homepage": "https://donmahallem.github.io/json-schema-action/",
