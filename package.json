  },
  "devDependencies": {
    "@donmahallem/eslint-config": "0.4.6",
    "@types/chai": "4.3.14",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.11",
    "@vercel/ncc": "0.32.0",
    "chai": "4.4.1",
    "commitizen": "4.2.4",
    "eslint": "8.3.0",
    "gulp": "4.0.2",
    "gulp-cli": "2.3.0",
    "mocha": "9.1.3",
    "nyc": "15.1.0",
    "prettier": "2.5.0",
    "ts-node": "10.4.0",
    "typedoc": "0.22.10",
    "typescript": "4.5.2"
  }
